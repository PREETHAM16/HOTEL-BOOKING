<html>
    <head>
        <title>Room Booking</title>
    </head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        body{
            text-align: center;
            background-image: url('bg.jpg');
        }
        .card{
            margin:5%;
        }
        .card:hover{
            position: relative;
            cursor: pointer;
            top: -4%;
        }
        .card-body{
            color:white;
            background: linear-gradient(90deg, rgb(0, 0, 0) 0%, rgb(27, 29, 32) 42%, rgb(47, 46, 48) 100%);
        }
        .list-group-item{
            background-color: rgb(94, 88, 88);
            color:white;
        }
.row{
    margin-left:2%;
}
<%  
    const monthNames=['01', '02', '03', '04', '05','06','07', '08', '09', '10', '11','12'] 
    const dateObj = new Date()
    const month = monthNames[dateObj.getMonth()]
    const day = String(dateObj.getDate()).padStart(2, '0')
    const year = dateObj.getFullYear()
    const begin=  year+'-'+month+'-'+day
    const days=7
    const date = new Date()
    const last = new Date(date.getTime() + (days * 24 * 60 * 60 * 1000))
    const dayL =String(last.getDate()).padStart(2,'0')
    const monthL=monthNames[last.getMonth()]
    const yearL=last.getFullYear()
    const end =  yearL+'-'+monthL+'-'+dayL
 %>
    </style>
    <body>

        <div class="container-fluid a">
            <h1 style="color:white">ROOM BOOKING</h1><br><br>
            <form action="/confirmationPage" method='post'>
                <table class="table container">
                    <div class="row ">
                        <tr style="background-color: black;color:white;">
                            <th>Name</th>
                            <th>Gmail</th>
                            <th>Mobile Number</th>
                            <th>Check-In</th>
                            <th>Check-Out</th>
                        </tr>
                    </div>
                <div class="row">
                        <td><input type="text" placeholder="Name" name='name' required></td>
                        <td><input type="email" placeholder="Gmail" name='gmail' required></td>
                        <td><input type="tel" placeholder="Mobile Number" name='mobileNumber' required></td>
                        <td><input type="date" min=<%= begin %> max=<%= end %> name='checkIn'></td>
                        <td><input type="date" min=<%= begin %> max=<%= end %> name='checkOut'></td>
                </table>

                <div class="row" >
                    <% for(let i=0;i<inse1.length;i++) { 
                        var a='Non AC';
                        if(inse1[i].roomTypeId==1)a='AC'  ;    
                    %>
                    <div class="col">
                        <div class="card" style="width: 18rem;margin:2%;height:100%">
                            <img class="card-img-top" src="room.jpeg" alt="Card image" style="height:150px">
                            <div class="card-body">
                            <h5 class="card-title"><%= inse1[i].variant+' '+a %> </h5>
                            <p class="card-text"><%= inse1[i].description %></p>
                            </div>
                            <ul class="card-body">
                                <li class="list-group-item" style="background-color:rgb(15, 124, 91)">Facilities</li>
                            <% let s=inse1[i].facilities.split(" ")
                                for(let j=0;j<s.length;j++) { %>
                            <li class="list-group-item"><%= s[j] %></li>
                            <% } %>
                            </ul>
                            <div class="card-body">
                            <a  class="card-link"><%= inse1[i].price %></a><br>
                            </div>
                            <h5 >Available Rooms <%= inse4[i] %></h5>
                            <input type='number' min=0  max=<%= inse4[i] %> name='room<%= i+1 %>' placeholder='Number Of Rooms (Available <%= inse4[i] %> )' style="height:12%;background-color:goldenrod;text-align:center" >
                        </div>
                    </div>
                    <% } %>
                </div><br>
                <button class="btn btn-success" style="width:15%;letter-spacing:0.2em;border-radius:25px;" type="submit">SUBMIT</button><br>
            </form>
        </div>
    </body>
    <script>
    </script>
</html>